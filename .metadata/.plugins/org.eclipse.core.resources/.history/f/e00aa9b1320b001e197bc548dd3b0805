package Objeto;

import java.io.IOException;

import javax.imageio.ImageIO;

import main.PanelJuego;

public class OBJ_Corazon extends SuperObjeto {
	
	PanelJuego pj;
	
	public OBJ_Corazon(PanelJuego pj) {
		
		this.pj = pj;
		
		nombre = "Corazon";
		try {
			
			imagen = ImageIO.read(getClass().getResourceAsStream("/Objetos/Llave.png"));
			
		}catch(IOException e) {
			
			e.printStackTrace();
			
		}
	}

}
